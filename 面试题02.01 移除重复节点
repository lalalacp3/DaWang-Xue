# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def removeDuplicateNodes(self, head: ListNode) -> ListNode:
        if not head:
            return 
        visited={head.val}                #记录已经访问过的
        cur=head
        while cur and cur.next:
            if cur.next.val in visited:      #如果在visited里面，那么这个值跳过
                cur.next = cur.next.next
            else:
                visited.add(cur.next.val)      #不在visited里面，visited加上这个值
                cur = cur.next
        return head
